{
    "name": "jed/test",
    "description": "Simple composer.json",
    "license": "MIT",
    "authors": [
        {
            "name": "Jed Bartolo",
            "email": "jedbartolo11@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "symfony/cache": "^6.3",
        "predis/predis": "^2.0"

    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "scripts": {
        "run-phpunit": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}